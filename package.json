  "license": "MIT",
  "scripts": {
    "lint": "eslint '**/*js'",
    "lint:fix": "prettier-eslint '**/*js' --write",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "client:dev": "rm -rf client/dist && webpack-dev-server --env.mode development --open --hot"
