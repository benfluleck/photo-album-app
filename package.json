    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.0.0",
    "react-styleguidist": "^9.0.5",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  }
